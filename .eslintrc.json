{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "semi": ["error", "always"],
    "indent": ["error", 2],
    "no-extra-semi": "warn",
    "require-jsdoc": "off"
  }
}
